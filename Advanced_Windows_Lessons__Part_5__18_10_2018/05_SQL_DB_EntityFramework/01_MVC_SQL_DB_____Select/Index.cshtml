@model IEnumerable<MVC_SQL_DB.Models.Employee>

		<div>
		<h2>Employees</h2>
			<table border="1">
				<tr>
                    <th>Id</th>
					<th>First Name</th>
					<th>Last Name</th>
					<th>Picture</th>
					</tr>
				@foreach (var m in Model)
				{
					<tr>
						<td>@m.id</td>
						<td>@m.firstName</td>
						<td>@m.lastName</td>
                        <td><img src="@m.picture"/></td>
					</tr>
				}
			</table>
		</div>
